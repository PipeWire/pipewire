"  -m, --midi                            Midi mode\n"
"  -d, --dsd                             DSD mode\n"
"  -o, --encoded                         Encoded mode\n"
"  -s, --sysex                           SysEx mode\n"
"  -c, --midi-clip                       MIDI clip mode\n"
"\n"
msgstr ""
"  -p, --playback                        Çalma kipi\n"
"  -r, --record                          Kayıt kipi\n"
"  -m, --midi                            Midi kipi\n"
"  -d, --dsd                             DSD kipi\n"
"  -o, --encoded                         Kodlanmış kip\n"
"  -s, --sysex                           SysEx kipi\n"
"  -c, --midi-clip                       MIDI klip kipi\n"
"\n"

